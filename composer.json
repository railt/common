{
    "name": "railt/common",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "Subtree split of the Railt Common component",
    "keywords": [
        "support",
        "helpers",
        "common",
        "php"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/common"
    },
    "require": {
        "php": ">=7.4",
        "railt/contracts": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Common\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Common\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "@php vendor/phpunit/phpunit/phpunit"
    },
    "extra": {
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev",
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
